import test::assert;

fn main() {
    assert(() is unit, "units should be units");
    assert(true is bool, "bools should be bools");
    assert('a' is char, "chars should be chars");
    assert(42 is int, "numbers should be integers");
    assert(42.1 is float, "numbers should be integers");
    assert("hello" is String, "strings should be strings");
    assert({} is Object, "objects should be objects");
    assert([] is Array, "arrays should be arrays");
}